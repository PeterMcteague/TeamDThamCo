networks: {}
services:
  basketservice:
    build:
      args:
        source: obj/Docker/empty/
      context: C:\Users\p4029206\Documents\GitHub\TeamDThamCo\BasketService
      dockerfile: Dockerfile
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
    image: basketservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$(pidof
        -x dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - '80'
    volumes:
    - C:\Users\p4029206\Documents\GitHub\TeamDThamCo\BasketService:/app:rw
    - C:\Users\p4029206\vsdbg:/remote_debugger:ro
  orderservice:
    build:
      args:
        source: obj/Docker/empty/
      context: C:\Users\p4029206\Documents\GitHub\TeamDThamCo\orderservice
      dockerfile: Dockerfile
    entrypoint: tail -f /dev/null
    environment:
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
    image: orderservice:dev
